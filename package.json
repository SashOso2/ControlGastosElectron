{
  "name": "pro-titi",
  "productName":"Control Gastos",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --platform=win32 --asar --arch=x64 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^33.0.1"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "electron-packager": "^17.1.2",
    "express": "^4.21.1",
    "sqlite3": "^5.1.7"
  }
}
